{
	"presets": [
		"@babel/typescript",
		"@babel/preset-react",
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": "last 2 versions"
				},
				"modules": false,
				"loose": false
			}
		]
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-object-rest-spread",
		"transform-class-properties"
	],
	"env": {
		"test": {
			"plugins": ["transform-es2015-modules-commonjs"]
		}
	}
}
